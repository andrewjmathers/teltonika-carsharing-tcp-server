{
  "name": "teltonika-carsharing-tcp-server",
  "version": "1.0.0",
  "description": "Teltonika tcp server for GPS devices",
  "scripts": {
    "forever": "forever start --minUptime 1000 --spinSleepTime 1000 server.js logs -f --watch",
